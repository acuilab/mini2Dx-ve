<?xml version="1.0"?>
<!DOCTYPE module PUBLIC "-//Checkstyle//DTD Checkstyle Configuration 1.3//EN" "https://checkstyle.org/dtds/configuration_1_3.dtd">
<module name="Checker">
	<property name="charset" value="UTF-8"/>
	<property name="severity" value="error"/>
	<module name="BeforeExecutionExclusionFileFilter">
		<property name="fileNamePattern" value="[Ss]erialized(Collection|Map).java$"/>
	</module>
	<module name="TreeWalker">
		<module name="IllegalImport">
			<property name="illegalClasses" value="java.util.List, java.util.Set, java.util.Map, java.util.ArrayList, java.util.HashSet, java.util.LinkedHashMap, java.util.LinkedHashSet, java.util.LinkedList"/>
		</module>
	</module>
</module>